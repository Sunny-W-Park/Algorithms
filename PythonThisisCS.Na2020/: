#4-1 상하좌우

n = int(input())
command = list(map(str, input().split()))
x, y = 1, 1

for k in range(len(command)):
    p = command[k]
    if n > x  and n > y:
        if p  == "R":
            y += 1
        elif p == "D":
            x += 1
    if x > 1 and y >= 1:
        if p  == "L":
            y -= 1
        elif p == "U":
            x -= 1

print(x, y)
